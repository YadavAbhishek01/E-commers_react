{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/componets/CartDetials.jsx", "src/componets/ProductList.jsx", "src/componets/Slider.jsx", "src/componets/Searchbar.jsx", "src/componets/NewProduct.jsx", "src/componets/NavIcons.jsx", "src/componets/Navbar.jsx", "src/componets/Loader.jsx", "src/componets/Footer.jsx", "src/componets/Filter.jsx", "src/componets/Category.jsx", "src/app/layout.jsx", "src/app/page.jsx", "src/app/login/page.jsx", "src/app/list/page.jsx", "src/app/[...slug]/page.jsx"],
  "exclude": ["node_modules"]
}
